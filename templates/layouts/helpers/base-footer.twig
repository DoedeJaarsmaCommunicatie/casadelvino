{% if fn('elementor_theme_do_location', 'footer') %}
{% endif %}


<script src="https://ajax.googleapis.com/ajax/libs/webfont/1.5.18/webfont.js"></script>
<script src="https://unpkg.com/quicklink@1.0.0/dist/quicklink.umd.js" async defer></script>
<script>
	WebFont.load({
		google: {
			families: [
                'Open Sans:300,400,700',
                'Playfair Display:400,900'
            ]
		}
	});

	window.addEventListener('load', () =>{
		quicklink();
	});
</script>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/TarekRaafat/autoComplete.js@3.2.2/dist/css/autoComplete.min.css" type="text/css" media="all">
<link rel="stylesheet" href="{{ site.theme.link }}/dist/styles/main.css" type="text/css" media="all">
<noscript>
</noscript>


<script>
	window.addEventListener("load", () => {
		document.body.appendChild(
				Object.assign(
						document.createElement(
								'link'
						),
						{
							type: "text/css",
							href: '{{ site.theme.link }}/dist/styles/main.css',
							rel: 'stylesheet',
							media: 'all'
						}
				)
		)
	})
</script>
